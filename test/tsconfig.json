{
    "compilerOptions": {
        "noEmit": true,
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "strictNullChecks": true,
        "jsx": "react",
        "lib": [
            "es5",
            "es2015",
            "es2015.promise",
            "es2015.iterable",
            "dom"
        ],
        "downlevelIteration": true,
        "declaration": true,
        "noImplicitThis": true
    }
}